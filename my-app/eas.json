{
  "cli": {
    "version": ">= 16.0.0",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "autoIncrement": true
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "developmentClient": false,
      "autoIncrement": true
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "developmentClient": false,
      "distribution": "store"
    }
  },
  "submit": {
    "production": {}
  }
}